{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "CardanoContext": "Host=localhost;Database=argus_identity;Username=postgres;Password=REPLACE_WITH_USER_SECRET;Port=5432",
    "CardanoContextSchema": "public"
  },
  "CardanoNodeConnection": {
    "ConnectionType": "UnixSocket",
    "UnixSocket": {
      "Path": "/path/to/node.socket"
    },
    "NetworkMagic": 2,
    "MaxRollbackSlots": 1000,
    "RollbackBuffer": 10,
    "Slot": 0,
    "Hash": "REPLACE_WITH_TIP_HASH"
  },
  "Sync": {
    "Dashboard": {
      "TuiMode": true,
      "RefreshInterval": 5000,
      "DisplayType": "sync"
    }
  }
}
